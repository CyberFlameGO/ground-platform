{
  "firestore": {
    "rules": "firestore/firestore.rules",
    "indexes": "firestore/firestore.indexes.json"
  },
  "hosting": {
    "public": "web-ng/dist/web-ng",
    "redirects": [
      {
        "source": "/",
        "destination": "/signin",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/importCsv",
        "function": "importCsv"
      },
      {
        "source": "/exportCsv",
        "function": "exportCsv"
      },
      {
        "source": "**/!(*.*)",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
